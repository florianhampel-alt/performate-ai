{
  "rewrites": [
    {
      "source": "/api/v1/analysis/(.*)",
      "destination": "https://performate-ai.onrender.com/analysis/$1"
    },
    {
      "source": "/api/v1/(.*)",
      "destination": "https://performate-ai.onrender.com/$1"
    },
    {
      "source": "/api/(.*)",
      "destination": "https://performate-ai.onrender.com/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://performate-ai.onrender.com",
    "NEXT_PUBLIC_APP_NAME": "Performate AI"
  }
}